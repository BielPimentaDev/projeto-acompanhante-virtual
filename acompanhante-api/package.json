{
	"name": "acompanhante-api",
	"version": "1.0.0",
	"description": "API REST TypeScript para o sistema de acompanhante virtual",
	"main": "dist/index.js",
	"engines": {
		"node": "20.x"
	},
	"scripts": {
		"build": "npm install && tsc",
		"start": "node dist/index.js",
		"dev": "ts-node-dev --respawn --transpile-only src/index.ts",
		"dev:watch": "nodemon --exec ts-node src/index.ts",
		"mock": "ts-node src/mock-client.ts",
		"mock:runner": "ts-node src/mock-runner.ts",
		"mock:build": "npm run build && node dist/mock-client.js",
		"clean": "rimraf dist",
		"test": "echo \"Error: no test specified\" && exit 1"
	},
	"keywords": [
		"express",
		"typescript",
		"api",
		"rest",
		"acompanhante-virtual"
	],
	"author": "",
	"license": "ISC",
	"dependencies": {
		"axios": "^1.11.0",
		"cors": "^2.8.5",
		"express": "^4.18.2",
		"helmet": "^7.0.0",
		"morgan": "^1.10.0",
		"typescript": "^5.1.6",
		"@types/node": "^20.5.0"
	},
	"devDependencies": {
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/morgan": "^1.9.4",
		"nodemon": "^3.0.1",
		"rimraf": "^5.0.1",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0"
	}
}
